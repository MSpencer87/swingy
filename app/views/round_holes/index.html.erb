<p id="notice"><%= notice %></p>

<h1>Listing Round Holes</h1>

<table>
  <thead>
    <tr>
      <th>Round</th>
      <th>Course</th>
      <th>Score</th>
      <th>Strokes</th>
      <th>Putts</th>
      <th>Notes</th>
      <th>Round</th>
      <th>Course hole</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @round_holes.each do |round_hole| %>
      <tr>
        <td><%= round_hole.round_id %></td>
        <td><%= round_hole.course_id %></td>
        <td><%= round_hole.score %></td>
        <td><%= round_hole.strokes %></td>
        <td><%= round_hole.putts %></td>
        <td><%= round_hole.notes %></td>
        <td><%= round_hole.round %></td>
        <td><%= round_hole.course_hole %></td>
        <td><%= link_to 'Show', round_hole %></td>
        <td><%= link_to 'Edit', edit_round_hole_path(round_hole) %></td>
        <td><%= link_to 'Destroy', round_hole, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Round hole', new_round_hole_path %>
